{
	"name": "99tests/99tests",
	"description": "A framework to generate valuable test cases automatically*",
	"type": "library",
	"keywords": [
		"php",
		"tests",
		"generator",
		"test cases"
	],	
    "require": {
		"php" : "^7.2",
		"ext-ds": "*",
        "symfony/dotenv": "v4.4.7",
		"symfony/yaml": "v4.4.7",
		"symfony/console": "v4.4.7",
		"symfony/phpunit-bridge": "v4.4.7",
		"squizlabs/php_codesniffer": "3.5.4",
      	"ext-json": "*"
    },
	"require-dev": {
    	"phpunit/phpunit": "7.5.14"
  	},
	"autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
